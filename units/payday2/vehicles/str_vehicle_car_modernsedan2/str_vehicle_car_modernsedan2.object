		<object name="g_body_lod0"				enabled="true" shadow_caster="true" />
		<object name="g_body_lod1"				enabled="false" />
		<object name="g_body_lod2" 				enabled="false" />
        <object name="g_glass_headlights" 		enabled="true"  />
        <object name="g_windows_side_right" 	enabled="true"  />
        <object name="g_windows_side_left" 		enabled="true"  />
