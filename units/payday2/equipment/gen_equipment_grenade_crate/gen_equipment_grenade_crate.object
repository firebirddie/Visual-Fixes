	</bodies>	
	
    <graphics>
		<object name="g_bottom_animated" 	enabled="true"/>
		<object name="g_bottom_static" 		enabled="true"/>

		<object name="g_seam" 	enabled="false" shadow_caster="true"/>
		<object name="g_lid" 	enabled="true" shadow_caster="true"/>
		<object name="g_lock" 	enabled="true" shadow_caster="true"/>
		<object name="g_hinge" 	enabled="true" shadow_caster="true"/>
